{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JwtSettings": {
    "Secret": "[YOUR-SECRET-KEY]",
    "Issuer": "[YOUR-ISSUER]",
    "Audience": "[YOUR-AUDIENCE]",
    "ExpiryMinutes": 15,
    "RefreshTokenExpiryDays": 1
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=[YOUR-DB-HOST];Port=5432;Database=[YOUR-DB-NAME];Username=[YOUR-DB-USER];Password=[YOUR-DB-PASSWORD]"
  },
  "MovieApi": {
    "BaseUrl": "https://phimapi.com",
    "ImageBaseUrl": "https://phimimg.com"
  }
}